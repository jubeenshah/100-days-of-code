    "import collections\n",
    "def repeatedString(s, n):\n",
    "    rep = s * n\n",
    "    count = 0\n",
    "    stop = n\n",
    "    for i in rep:\n",
    "        \n",
    "        if i == \"a\" and stop >=0:\n",
    "            count += 1\n",
    "        stop -= 1\n",
    "    print(count)\n",
    "    return count\n",
    "repeatedString(\"aba\",10)"
