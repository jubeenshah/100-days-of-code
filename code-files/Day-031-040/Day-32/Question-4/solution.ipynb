   "source": [
    "from operator import add\n",
    "def pascalsTriangleDos(rowIndex):\n",
    "    result = [[1]]\n",
    "    for _ in range(rowIndex):\n",
    "        map_ = map(add, [0] +result[-1],result[-1]+[0])\n",
    "        result.append(list(map_))\n",
    "    return result[rowIndex] if rowIndex else [[1]]\n",
    "pascalsTriangleDos(2)"
   ]
