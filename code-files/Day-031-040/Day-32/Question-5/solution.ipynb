    "dictOfFactorials = {}\n",
    "def factorials(n):\n",
    "    #print(dictOfFactorials)\n",
    "    if n in dictOfFactorials:\n",
    "        return dictOfFactorials[n]\n",
    "    if n == 0 or n == 1:\n",
    "        return 1\n",
    "    dictOfFactorials[n] = n * factorials(n-1)\n",
    "    return (n * factorials(n-1))\n",
    "factorials(203)"
