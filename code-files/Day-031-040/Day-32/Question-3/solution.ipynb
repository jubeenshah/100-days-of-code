   "source": [
    "from operator import add\n",
    "def pascalsTriangle(numRows):\n",
    "    res = [[1]]\n",
    "    for _ in range(1, numRows):\n",
    "        map_ = map(add, [0] + res[-1], res[-1] + [0])\n",
    "        res.append(list(map_))\n",
    "    return res if numRows else []\n",
    "        \n",
    "    \n",
    "\n",
    "pascalsTriangle(5)"
   ]
