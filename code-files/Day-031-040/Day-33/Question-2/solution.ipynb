    "import math\n",
    "def squares(a, b):\n",
    "    counter = 0\n",
    "    start = int(math.sqrt(a))\n",
    "    end = int(math.sqrt(b))\n",
    "    for i in range(start,end+1):\n",
    "        if i**2 >= a and i**2 <= b:\n",
    "            counter += 1\n",
    "    return counter\n",
    "\n",
    "squares(465868129, 988379794)"
