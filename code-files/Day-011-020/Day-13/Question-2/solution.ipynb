    "def removeDuplicates(arr):\n",
    "    i = 0\n",
    "    while i < len(arr) -1:\n",
    "        #print(arr, i, arr[i])\n",
    "        if arr[i] == arr[i+1]:\n",
    "            del arr[i+1]\n",
    "            i -=1\n",
    "        i += 1\n",
    "    return len(arr)\n",
    "removeDuplicates([0,0,1,1,1,2,2,3,3,4])"
   ]
