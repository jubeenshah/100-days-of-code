    "def lengthOfLastWord(s:str)->int:\n",
    "    s = s.split(' ')\n",
    "    #print(s,len(s[-1]))\n",
    "    returnLen = 0\n",
    "    for i in range(len(s)-1,-1,-1):\n",
    "        #print(s[i],i)\n",
    "        if s[i] != '':\n",
    "            return len(s[i])\n",
    "    return returnLen\n",
    "lengthOfLastWord(\"   123 \")"
