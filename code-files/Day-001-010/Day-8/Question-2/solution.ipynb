    "def twoSum(nums,target):\n",
    "    hash_map = {}\n",
    "    for idx, val in enumerate(nums):\n",
    "        new_val = target - val\n",
    "        if new_val in hash_map:\n",
    "            return hash_map[new_val], idx\n",
    "        else:\n",
    "            hash_map[val] = idx\n",
    "\n",
    "twoSum([3,3], 6)"
   ]
  }
