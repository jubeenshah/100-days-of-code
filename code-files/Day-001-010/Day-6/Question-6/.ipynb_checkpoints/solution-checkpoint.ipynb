    "def flippingBits(n):\n",
    "    binary = '{:032b}'.format(n)\n",
    "    newBinary = ''\n",
    "    for bit in binary:\n",
    "        if bit == '1': \n",
    "            newBinary += '0' \n",
    "        else: \n",
    "            newBinary += '1'\n",
    "    return int(newBinary,2)\n",
    "flippingBits(0)"
   ]
  }
