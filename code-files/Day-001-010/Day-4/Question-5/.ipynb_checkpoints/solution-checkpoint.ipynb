   "source": [
    "def plusMinus(arr):\n",
    "    pos = 0\n",
    "    neg = 0\n",
    "    zer = 0\n",
    "    \n",
    "    for a in arr:\n",
    "        if a < 0:\n",
    "            neg += 1\n",
    "        elif a > 0:\n",
    "            pos += 1\n",
    "        else:\n",
    "            zer += 1\n",
    "    return (pos/len(arr), neg/len(arr), zer/len(arr))\n",
    "\n",
    "plusMinus([-4, 3, -9, 0, 4, 1])"
   ]
