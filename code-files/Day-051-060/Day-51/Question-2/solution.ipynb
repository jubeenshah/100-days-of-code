    "def printPallindrome(n):\n",
    "    for i in range(1,n+1):\n",
    "        stringToPrint = \"\"\n",
    "        for j in range(1,i+1):\n",
    "            stringToPrint += str(j)\n",
    "        for k in range(i-1,0,-1):\n",
    "            stringToPrint += str(k)\n",
    "        print(stringToPrint)\n",
    "\n",
    "printPallindrome(int(input()))"
   ]
