    "def howManyGames(p, d, m, s):\n",
    "    numberOfGames = 0\n",
    "    while s >= 0:\n",
    "        s = s - p\n",
    "        #if s > 0:\n",
    "        numberOfGames += 1\n",
    "        p = max(p - d, m)\n",
    "    return numberOfGames - 1\n",
    "\n",
    "howManyGames(20, 3, 6, 80)"
   ]
